{
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb-typescript",
    "plugin:tailwindcss/recommended"
  ],
  "parserOptions": {
    "project": "tsconfig.json"
  },
  "rules": {
    "tailwindcss/classname-order": "off",
    "react/react-in-jsx-scope": "off",
    "@typescript-eslint/naming-convention": "warning"
  }
}
